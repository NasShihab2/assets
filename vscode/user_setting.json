{
    "redhat.telemetry.enabled": true,
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            160
        ],
        "editor.selectionHighlight": false,
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "workbench.iconTheme": "material-icon-theme",
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "Menlo",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.lineHeight": 23,
    "editor.fontWeight": "400",
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "smooth",
    "bracketPairColorizer.depreciation-notice": false,
    "workbench.auxiliaryActivityBar.location": "default",
    "antigravity.marketplaceGalleryItemURL": "",
    "antigravity.marketplaceExtensionGalleryServiceURL": "",
    "security.workspace.trust.untrustedFiles": "open",
    "dart.openDevTools": "flutter",
    "dart.debugExternalPackageLibraries": false,
    "dart.debugSdkLibraries": false,
    "dart.flutterTrackWidgetCreation": false,
    "debug.console.fontSize": 11,
    "workbench.colorCustomizations": {
        "debugConsole.infoForeground": "#FFFFFF",
        "debugConsole.sourceForeground": "#569CD6",
        "debugConsole.warningForeground": "#CCA700",
        "debugConsole.errorForeground": "#F48771"
    },
    "explorer.confirmDragAndDrop": false,
    "window.zoomLevel": 0.7,
    "git.autofetch": true,
    "diffEditor.codeLens": true,
    "editor.foldingStrategy": "auto",
    "editor.foldingImportsByDefault": true,
    "editor.showFoldingControls": "always",
    "workbench.colorTheme": "One Dark Pro Night Flat",
    "editor.minimap.size": "fill",
    "editor.minimap.maxColumn": 40
}
